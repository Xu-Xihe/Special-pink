	"tokenColors":[
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#ff00aa"
			}
		},
		{
			"name": "Comment",
			"scope":[
				"comment.line",
				"comment.block",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#a8a8a8",
				"fontStyle": "italic underline"
			}
		},
		{
			"name": "Class",
			"scope":[
				"entity.name.type",
				"entity.other.inherited-class",
